<!DOCTYPE html>
<html>
<head>
  <title>Ticketee</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="container">
		<div class="content">
			<div id="main">
				<% flash.each do |key, value| %>
					<div id="<%= key %>"> <%= value %> </div>
				<% end %>
				<%= yield %>
			</div>
		</div>
	</div>
</body>
</html>
